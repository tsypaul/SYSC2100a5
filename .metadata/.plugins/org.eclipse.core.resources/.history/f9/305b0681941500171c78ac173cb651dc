package part1;

public class HashTable<K, V> {
	private int size = 0;
	// capitalized letter and non-capitalized letter 26*2 = 52
	private int tableSize = 52;
	private ChainNode[] table;
	
	public HashTable(){
		table = new ChainNode[tableSize];
	}
	
	public boolean tableIsEmpty(){
		return size == 0;
	}
	
	public int tableSize(){
		return tableSize;
	}
	
	public void tableInsert(K key, V value) throws RuntimeException{
		
	}
	
	public boolean tableDelete(){
		return true;
	}
	
	public V tableRetrieve(K key){
		return null;
	}
	
	public int hashIndex(K key){
		return 1;
	}
}
